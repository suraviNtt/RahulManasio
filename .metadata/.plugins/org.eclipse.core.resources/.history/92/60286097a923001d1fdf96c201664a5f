package oppsBasic;

public class A {

	public int i = 10;

	public void test() {
		System.out.println("Public method");
	}

	// overloading
	public void test(int i) {

	}

	// overloading
	public void test(int j, int i) {

	}

	// overloading
	public void test(String i) {

	}

	public static void main(String[] args) {

		A a = new A();
		a.test();
	}

}
