package oppsBasic;

public class A {

	public int i = 10;

	public void test() {
		System.out.println("Public method");
	}

	// overloading
	public void test(int i) {
		System.out.println(i);
	}
	
	// overloading
	public void test(int j, int i) {
		System.out.println( i + " "+j);
	}

	// overloading
	public void test(String i) {
		System.out.println(i);
	}

	public static void main(String[] args) {

		A a = new A();
		a.test();
	}

}
