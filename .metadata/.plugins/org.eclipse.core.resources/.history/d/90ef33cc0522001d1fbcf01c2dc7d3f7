package conditionalOperations;

public class H {

	public static void main(String[] args) {
		
		int i = 1;
		
		i++ ; //1
		System.out.println(i); //2
		i--;//2
		System.out.println(i); //1
		++i;//2
		System.out.println(i);//2
		--i;//1
		System.out.println(i);//1
		
		
		int y = i++ + i-- +i++ + i++;
		System.out.println(y);
	}
}
