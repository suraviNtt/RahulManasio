package airthmanticOperations;

public class AddingNumberBymethod {

	
	public void add(int x , int y) {
		int i=x;
		int j = y;
		int sum = i+j;
		System.out.println(sum);
	}
	
	public static void main(String[] args) {
		AddingNumberBymethod obj = new AddingNumberBymethod();
		obj.add(10,20);
	}
}
