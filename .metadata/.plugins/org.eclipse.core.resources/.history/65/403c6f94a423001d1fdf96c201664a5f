package OPPsbasic;

import java.security.DomainCombiner;

public class Dog {

	int no_of_legs = 4;
	String type = "BullDog";
	int no_ears = 2;
	
	public static void main(String[] args) {
		
		Dog dog1 = new Dog();
		Dog dog2 = new Dog();
		Dog dog3 = new Dog();
		Dog dog4 = new Dog();
		
		System.out.println(dog1.type);
		System.out.println(dog2.no_of_legs);
		System.out.println(dog3.no_ears);
		System.out.println(dog4.type);
		
	}
}
